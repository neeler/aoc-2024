{
  "name": "@neeler/aoc-2024",
  "version": "1.0.0",
  "description": "Solutions for Advent of Code 2024",
  "scripts": {
    "generate": "node -r @swc-node/register src/scripts/generate.ts",
    "dev": "nodemon ./src/index.ts",
    "setup": "node -r @swc-node/register src/scripts/setup.ts",
    "prebuild": "npm run clean",
    "build": "swc src -d dist & npm run copyAssets",
    "clean": "npx rimraf dist",
    "copyAssets": "node -r @swc-node/register src/scripts/copyAssets.ts",
    "prestart": "npm run build",
    "start": "node ./dist/src/index.js",
    "postinstall": "npm run setup",
    "lint:es": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "neeler",
  "license": "ISC",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "volta": {
    "node": "22.11.0",
    "npm": "10.9.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.5.1",
    "@swc/core": "^1.9.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.10.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "kleur": "^4.1.5"
  }
}
